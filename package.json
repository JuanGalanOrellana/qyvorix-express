    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.8.1",
    "@types/cookie": "^0.6.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.3",
    "@types/generate-password": "^1.5.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.4",
    "@types/otplib": "^7.0.0",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "cross-env": "^7.0.3",
    "jest": "^30.0.4",
    "supertest": "^7.1.3",
    "swagger-typescript-api": "^13.2.7",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-standard": "^12.0.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "body-parser": "2.2.0",
    "compression": "1.8.1",
