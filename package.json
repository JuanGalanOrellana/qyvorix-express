  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "lint": "ts-standard",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "node build/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "test": "jest --detectOpenHandles",
