    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "resend": "^6.6.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "./node_modules/ts-standard/eslintrc.json",
      "prettier"
    ]
  }
